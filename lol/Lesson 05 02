<meta name="color-scheme" content="dark">

<!-- Start writing JavaScript code -->

<script>

// Create the variables at the top of the program
var a = 1;
var b = 2;


// Create "c" which uses "b" to calculate a number
var c = b * b * b;
// Now we can use "c" in the program and it will equal 8
// c = 2 * 2 * 2 = 8


console.log(a + b);
// The result will be 3

console.log(b - a);
// The result will be 1

console.log(b * b);
// The result will be 4

console.log(b * b * b);
// The result will be 8

console.log(c + b * b * b);
// The result will be 16


</script>

