<!-- Start writing JavaScript code -->
<script>

var a = 1;
var b = 1;
var c = 2;

console.log("a=b", a == b); 
// The output is TRUE

console.log("a=c", a == c); 
// The output is FALSE because "a" and "c" are not the same


</script>

